%
% generating PC fonts (example for MFjob)
% for 600dpi HP laser printer (BJ & MR)
%
input [modes];
{
fonts=pccsc#  % concrete caps and small caps
      pcr#    % concrete roman
      pcti#   % concrete text italic
      pcsl#   % concrete slanted
      pcslc#; % concrete slanted condensed
ljh;
{mag=s0;}
}

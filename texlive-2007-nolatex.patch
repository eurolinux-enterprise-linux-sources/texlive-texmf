+FUNCTION {mult}
+{ 'a :=
+  'b :=
+  b #0 <
+    {#-1 #0 b - 'b :=}
+    {#1}
+  if$
+  #0
+  {b #0 >}
+    { a +
+      b #1 - 'b :=
+    }
+  while$
+  swap$
+    'skip$
+    {#0 swap$ -}
+    if$
